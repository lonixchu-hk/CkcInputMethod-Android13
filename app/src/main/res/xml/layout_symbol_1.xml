<?xml version="1.0" encoding="utf-8"?>
<Keyboard
    xmlns:app="http://schemas.android.com/apk/res-auto"
    app:keyWidth="8.9%p"
    app:horizontalGap="1%p"
    app:verticalGap="20px"
    app:keyHeight="42dp"
    app:keyBackground="@drawable/key_background" >

    <Row app:keyHeight="2dp"/>

    <Row>
        <Key app:codes="49" app:keyLabel="1"/>
        <Key app:codes="50" app:keyLabel="2"/>
        <Key app:codes="51" app:keyLabel="3"/>
        <Key app:codes="52" app:keyLabel="4"/>
        <Key app:codes="53" app:keyLabel="5"/>
        <Key app:codes="54" app:keyLabel="6"/>
        <Key app:codes="55" app:keyLabel="7"/>
        <Key app:codes="56" app:keyLabel="8"/>
        <Key app:codes="57" app:keyLabel="9"/>
        <Key app:codes="48" app:keyLabel="0"/>
    </Row>

    <Row>
        <Key app:codes="64"  app:keyLabel="@"/>
        <Key app:codes="35" app:keyLabel="#"/>
        <Key app:codes="37" app:keyLabel="%"/>
        <Key app:codes="95" app:keyLabel="_"/>
        <Key app:codes="38" app:keyLabel="&amp;"/>
        <Key app:codes="45" app:keyLabel="-"/>
        <Key app:codes="43" app:keyLabel="+"/>
        <Key app:codes="40" app:keyLabel="("/>
        <Key app:codes="41" app:keyLabel=")"/>
        <Key app:codes="47" app:keyLabel="/"/>
    </Row>

    <Row>
        <Key app:codes="-14" app:keyLabel="1/2" app:keyWidth="15%p" app:keyEdgeFlags="left" app:isModifier="true" />
        <Key app:codes="42" app:keyLabel="*" />
        <Key app:codes="34" app:keyLabel="&quot;" />
        <Key app:codes="39" app:keyLabel="'" />
        <Key app:codes="58" app:keyLabel=":" />
        <Key app:codes="59" app:keyLabel=";" />
        <Key app:codes="33" app:keyLabel="!" />
        <Key app:codes="63" app:keyLabel="?" />
        <!--        <Key app:codes="-5" app:keyIcon="@drawable/ic_backspace" app:keyWidth="20%p" />-->
        <Key app:codes="-5" app:keyLabel="BK" app:keyWidth="15%p" app:isRepeatable="true" />
    </Row>

    <Row>
        <Key app:codes="-11" app:keyLabel="ä¸­" app:keyWidth="15%p" app:keyEdgeFlags="left" />
        <!--        <Key app:codes="-7" app:keyIcon="@drawable/ic_123" />-->
        <!--        <Key app:codes="32" app:keyIcon="@drawable/ic_spacebar" app:keyWidth="50%p" />-->
        <Key app:codes="-12" app:keyLabel="ABC" app:keyWidth="15%p"/>
        <Key app:codes="32" app:keyLabel="" app:keyWidth="33%p" />
        <Key app:codes="44" app:keyLabel="," />
        <Key app:codes="46" app:keyLabel="." />
        <Key app:codes="-4" app:keyLabel="Enter" app:keyWidth="15%p"/>
    </Row>

    <Row app:keyHeight="2dp"/>


</Keyboard>